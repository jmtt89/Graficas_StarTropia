#include "../include/staticAgent.h"staticAgent::staticAgent(){	std::cout << "Constructor static Agent" << std::endl;	this->x = 0.0f;	this->y = 0.0f;	this->z = 0.0f;	this->Alive = false;}staticAgent::~staticAgent(){	std::cout << "Destructor de static Agent" << std::endl;		}void staticAgent::Assign(GLfloat x,GLfloat y,GLfloat z){	std::cout << "Asignador static Agent" << std::endl;	this->x = x;	this->y = y;	this->z = z;	this->Alive = true;}void staticAgent::Update(){	std::cout << "Update static Agent" << std::endl;}void staticAgent::Draw(){	std::cout << "Draw static Agent" << std::endl;}bool staticAgent::Collision(){	std::cout << "Collision static Agent" << std::endl;	return false;}